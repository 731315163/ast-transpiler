{
  "name": "transpiler",
  "type": "module",
  "module": "./dist/transpiler.js",
  "types": "./dist/transpiler.d.ts",
  "exports": {
    ".": {
      "require": "./dist/transpiler.js",
      "import": "./dist/transpiler.js",
      "types": "./dist/transpiler.d.ts"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "readme": "README.md",
  "scripts": {
    "build": "npm run lint &&  tsup src/transpiler.ts --format cjs,esm --dts --clean",
    "build2": "npm run lint && tsc",
    "start": "node --loader ts-node/esm src/manual.ts",
    "start-py": "node --loader ts-node/esm src/pythonTranspiler.ts",
    "start2": " ts-node-esm src/transpiler.ts",
    "test-ci": "jest --ci --coverage",
    "test": "jest --verbose",
    "testv": "jest --verbose --coverage",
    "test2": "node --experimental-vm-modules node_modules/.bin/jest",
    "test3": "NODE_OPTIONS=--experimental-vm-modules jest --verbose",
    "test4": "node --loader ts-node/esm --experimental-specifier-resolution=node src/manual.ts",
    "lint": "eslint src/ --ext .ts"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^18.11.4",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "test-jest": "^1.0.1",
    "ts-jest": "^29.0.3",
    "ts-morph": "^16.0.0",
    "ts-node": "^10.9.1",
    "tsup": "^6.4.0"
  },
  "dependencies": {
    "typescript": "^4.8.4"
  }
}
